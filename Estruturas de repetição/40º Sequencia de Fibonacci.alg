algoritmo "Sequencia de Fibonacci"
var
Contador, Fibonacci, Soma: Inteiro
inicio
Soma <- 1
Fibonacci <- 0
   Para Contador <- 1 ate 7 faca
        Escreva (Fibonacci)
        Fibonacci <- Fibonacci+Soma
        Escreva (Soma)
        Soma <- Soma+Fibonacci
   FimPara
   Escreva (Fibonacci)
fimalgoritmo
